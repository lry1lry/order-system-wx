page,view,text,swiper,swiper-item,image,navigator{
  padding:0;
  margin: 0;
  box-sizing: border-box;
 }

em,
i {
	font-style: normal
}

li {
	list-style: none
}

image {
	border: 0;
	vertical-align: middle
}

button {
	cursor: pointer
}

navigator {
	color: black;
	text-decoration: none
}

navigator:hover {
	color: #c81623
}

button,
input {
	font-family: Microsoft YaHei, Heiti SC, tahoma, arial, Hiragino Sans GB, "\5B8B\4F53", sans-serif;
	border:0;
	outline: none;
}

body {
	-webkit-font-smoothing: antialiased;
	background-color: #fafafa;
	font: 25rpx Microsoft YaHei, Heiti SC, tahoma, arial, Hiragino Sans GB, "\5B8B\4F53", sans-serif;
	color: black
}

.hide,
.none {
	display: none
}

.clearfix:after {
	visibility: hidden;
	clear: both;
	display: block;
	content: ".";
	height: 0
}

.clearfix {
	zoom: 1;
}
